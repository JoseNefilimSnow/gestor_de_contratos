<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent" android:layout_height="match_parent" android:visibility="visible">
    <TextView
            android:text="@string/nombre_de_la_empresa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignParentStart="true"
            android:layout_marginStart="30dp" android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
            android:id="@+id/textView" android:layout_marginTop="45dp" android:layout_marginLeft="30dp"
            android:visibility="visible" android:textColor="@android:color/background_dark"
            android:layout_alignParentEnd="true" android:layout_alignParentRight="true"
            android:layout_marginRight="208dp" android:layout_marginEnd="208dp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/nombreEmpresa"
            android:layout_alignStart="@+id/textView" android:layout_marginStart="2dp"
            android:layout_marginTop="17dp" android:layout_below="@+id/textView" android:hint="@string/empresa"
            android:importantForAutofill="no" android:visibility="visible"/>
    <TextView
            android:text="@string/tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignParentTop="true" android:id="@+id/textView2"
            android:layout_marginTop="152dp" android:layout_alignStart="@+id/textView"
            android:layout_alignParentEnd="true" android:layout_alignParentRight="true"
            android:layout_marginRight="323dp" android:layout_marginEnd="323dp"
            android:textColor="@android:color/background_dark" android:visibility="visible"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText" android:layout_marginTop="18dp"
            android:layout_alignStart="@+id/textView" android:layout_below="@+id/textView2" android:hint="@string/tipo2"
            android:visibility="visible" android:autofillHints=""/>
    <TextView
            android:text="@string/fecha_de_inicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:layout_alignStart="@+id/textView"
            android:textColor="@android:color/background_dark" android:visibility="visible"
            android:layout_marginTop="261dp" android:layout_marginStart="4dp" android:layout_alignParentTop="true"/>
    <TextView
            android:text="@string/fecha_de_fin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:layout_alignStart="@+id/textView"
            android:textColor="@android:color/background_dark" android:visibility="visible"
            android:layout_marginTop="360dp" android:layout_marginStart="3dp" android:layout_alignParentTop="true"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:layout_alignEnd="@+id/editText" android:layout_alignParentBottom="true"
            android:id="@+id/fecha_inicio"
            android:layout_marginBottom="224dp" android:layout_marginTop="17dp" android:layout_below="@+id/textView3"
            android:layout_marginEnd="-1dp" android:hint="@string/a_o_mes_d_a" android:importantForAutofill="no"
            android:visibility="visible"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:layout_alignEnd="@+id/editText" android:layout_alignParentBottom="true"
            android:id="@+id/fecha_fin"
            android:layout_marginBottom="122dp" android:layout_marginTop="17dp" android:layout_below="@+id/textView4"
            android:layout_marginEnd="-3dp" android:hint="@string/a_o_mes_d_a2" android:autofillHints=""
            android:visibility="visible"/>
</RelativeLayout>