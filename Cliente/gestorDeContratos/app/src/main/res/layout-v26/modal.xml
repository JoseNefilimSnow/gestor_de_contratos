<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent" android:layout_height="match_parent" android:visibility="visible">
    <TextView
            android:text="@string/nombre_de_la_empresa"
            android:layout_width="196dp"
            android:layout_height="wrap_content" android:layout_alignParentStart="true"
            android:layout_marginStart="28dp" android:layout_alignParentLeft="true" android:layout_alignParentTop="true"
            android:id="@+id/textView" android:layout_marginTop="28dp" android:layout_marginLeft="28dp"
            android:visibility="visible" android:textColor="@android:color/background_dark"
            android:layout_alignParentEnd="true" android:layout_alignParentRight="true"
            android:layout_marginRight="136dp" android:layout_marginEnd="136dp" android:textSize="18sp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="text"
            android:ems="10"
            android:id="@+id/nombreEmpresa"
            android:layout_alignStart="@+id/textView" android:layout_marginStart="-1dp"
            android:layout_marginTop="7dp" android:layout_below="@+id/textView" android:hint="@string/empresa"
            android:importantForAutofill="no" android:visibility="visible"/>
    <TextView
            android:text="@string/tipo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView2"
            android:layout_marginTop="14dp" android:layout_alignStart="@+id/textView"
            android:layout_alignParentEnd="true" android:layout_alignParentRight="true"
            android:layout_marginRight="279dp" android:layout_marginEnd="279dp"
            android:textColor="@android:color/background_dark" android:visibility="visible"
            android:layout_below="@+id/nombreEmpresa" android:layout_marginStart="2dp" android:textSize="18sp"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/editText" android:layout_marginTop="6dp"
            android:layout_alignStart="@+id/textView" android:layout_below="@+id/textView2" android:hint="@string/tipo2"
            android:visibility="visible" android:autofillHints="" android:layout_marginStart="1dp"/>
    <TextView
            android:text="@string/fecha_de_inicio"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView3"
            android:layout_alignStart="@+id/textView"
            android:textColor="@android:color/background_dark" android:visibility="visible"
            android:layout_marginTop="14dp" android:layout_marginStart="2dp" android:layout_below="@+id/editText"
            android:textSize="18sp"/>
    <TextView
            android:text="@string/fecha_de_fin"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView4"
            android:textColor="@android:color/background_dark" android:visibility="visible"
            android:layout_marginTop="12dp" android:layout_below="@+id/fecha_inicio"
            android:layout_alignStart="@+id/textView" android:textSize="18sp"/>
    <EditText
            android:layout_width="176dp"
            android:layout_height="39dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:layout_alignEnd="@+id/editText"
            android:id="@+id/fecha_inicio"
            android:layout_marginTop="7dp" android:layout_below="@+id/textView3"
            android:layout_marginEnd="37dp" android:hint="@string/dia_mes_a_o" android:importantForAutofill="no"
            android:visibility="visible"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="53dp"
            android:inputType="textPersonName"
            android:ems="10"
            android:id="@+id/fecha_fin"
            android:layout_marginTop="5dp" android:layout_below="@+id/textView4"
            android:hint="@string/dia_mes_a_o" android:autofillHints=""
            android:visibility="visible" android:layout_alignParentStart="true" android:layout_marginLeft="31dp"
            android:layout_marginStart="31dp" android:layout_alignParentLeft="true"/>
    <Button
            android:text="@string/cerrar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:layout_alignParentBottom="true" android:id="@+id/cerrarCrearContrato"
            android:layout_marginBottom="43dp" android:layout_alignParentStart="true" android:layout_marginLeft="32dp"
            android:layout_marginStart="32dp" android:layout_alignParentLeft="true"
            android:background="@android:color/holo_red_dark" android:textColor="@android:color/background_light"/>
    <Button
            android:text="@string/continuar"
            android:layout_width="115dp"
            android:layout_height="wrap_content"
            android:id="@+id/continuar" android:layout_alignParentBottom="true" android:layout_marginStart="94dp"
            android:layout_marginBottom="41dp" android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true" android:layout_marginRight="31dp" android:layout_marginEnd="31dp"
            android:layout_toEndOf="@+id/cerrarCrearContrato" android:layout_toRightOf="@+id/cerrarCrearContrato"
            android:layout_marginLeft="94dp" android:textColor="@android:color/background_light"
            android:background="@android:color/holo_green_dark"/>
</RelativeLayout>