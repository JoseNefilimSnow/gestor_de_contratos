<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="com.loquat.jose.gestor_de_contratosv3">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher_round"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true"
            tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".AddContratoActivity"
                android:label="Añadir Contrato..."/>
        <activity
                android:name=".ListaContratosEmpresa"
                android:label="Detalles de la Empresa"/>
        <activity
                android:name=".AddEmpresa"
                android:label="Añadir Empresa..."/>
        <activity android:name=".ListaCategorias"
                  android:label="Categorias de Servicios"/>
    </application>

</manifest>